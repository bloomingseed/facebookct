{
    "manifest_version": 3,
    "name": "Facebook Commentor Tool",
    "description": "A tool to auto post comment to facebook posts and update the spreadsheet status. This extension works together with the designed Google app script.",
    "version": "1.0.0",
    "background":{
        "service_worker": "background-wrapper.js"
    },
    "icons":{
        "16": "/images/logo_16.png",
        "128":"/images/logo_128.png"
    },
    "action":{
        "default_icon":{
            "16": "/images/logo_16.png",
            "128":"/images/logo_128.png"
        },
        "default_popup":"/popup.html"
    },
    "permissions":[
        "tabs",
        "scripting",
        "storage",
        "identity"
    ],
    "host_permissions":[
        "https://www.facebook.com/*"
    ],
    "oauth2": {
        "client_id": "261045199742-cf109oo62ls6t1gfuld03mljn8pj1onp.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/spreadsheets",
            "https://www.googleapis.com/auth/script.scriptapp"
        ]
    },
    "key":""
}